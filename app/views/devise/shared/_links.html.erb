<%- if controller_name != 'sessions' %>
  <%= link_to t('link.sign_in'), new_session_path(resource_name), class: "link_log" %><br />
<% end -%>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to t('link.sign_up'), new_registration_path(resource_name),class: "link_logi" %><br />
<% end -%>

<%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
  <%= link_to t('link.forgot'), new_password_path(resource_name),class: "link_logi" %><br />
<% end -%>

<%- if devise_mapping.confirmable? && controller_name != 'confirmations' %>
  <%= link_to t('link.receive_confirm'), new_confirmation_path(resource_name),class: "link_log" %><br />
<% end -%>

<%- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks' %>
  <%= link_to t('link.receive_unlock'), new_unlock_path(resource_name), class: "link_log" %><br />
<% end -%>

<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
    <%= link_to "Sign in with #{OmniAuth::Utils.camelize(provider)}", omniauth_authorize_path(resource_name, provider),class: "link_log" %><br />
  <% end -%>
<% end -%>
